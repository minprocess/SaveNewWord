{
  "name": "French Words Side Panel",
  "manifest_version": 3,
  "version": "0.1",
  "description": "Chrome extension side panel to enter French words into a database.",
  "permissions": ["sidePanel", "storage", "tabs"],
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}